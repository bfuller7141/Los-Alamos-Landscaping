---
interface BaseHeadProps {
  title: string;
  description: string;
  keywords: string;
  image: string;
  url: string;
}

const {title, description, keywords, image, url} = Astro.props as BaseHeadProps;
---
<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<title>{title || "Los Alamos Landscaping & More"}</title>
<meta name="description" content={description || "Los Alamos Landscaping & More"} />
<meta name="keywords" content={keywords || "landscaping, los alamos, new mexico"} />

<link rel="canonical" href={url || "https://losalamoslandscaping.com"} />

<!-- Favicons -->

<!-- Styles -->
<link rel="stylesheet" href="/styles/global.css" />
<link rel="stylesheet" href="/styles/root.css" />

<!-- Fonts -->
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Rubik:ital,wght@0,300..900;1,300..900&display=swap" rel="stylesheet">